{
  "configurations": {
    "run": {
      "adapter": "debugpy",
      "configuration": {
        "request": "launch",
        "type": "python",
        "cwd": "${workspaceRoot}",
        "program": "main.py",
        "args": [
          "--name",
          "default008",
          "--base",
          "configs/train.yaml",
          "--train",
          "--gpus",
          "0,"
        ],
        "stopOnEntry": true,
        "console": "integratedTerminal",
        "env": {
          "FOO": "123"
        },
        "justMyCode": false
      },
      "breakpoints": {
        "exception": {
          "raised": "N",
          "uncaught": "",
          "userUnhandled": ""
        }
      }
    }
  }
}
